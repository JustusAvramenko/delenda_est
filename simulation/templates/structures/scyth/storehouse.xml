<?xml version="1.0" encoding="utf-8"?>
<Entity parent="autobuild|template_structure_economic_storehouse">
  <Builder>
    <Rate>1.0</Rate>
    <Entities datatype="tokens">
      structures/{civ}/field
      structures/{civ}/field_cash_crops
    </Entities>
  </Builder>
  <BuildRestrictions>
    <Territory>own neutral ally enemy</Territory>
  </BuildRestrictions>
  <Footprint replace="">
    <Circle radius="8"/>
    <Height>10</Height>
  </Footprint>
  <Identity>
    <Civ>scyth</Civ>
    <Tooltip>Dropsite for all resources. Train Slaves. Upgrade to a Resource Wagon to move this Storehouse across the map.</Tooltip>
  </Identity>
  <Researcher>
    <Technologies datatype="tokens" replace="">
      nomads/gather_wicker_baskets
	  nomads/gather_hunting_dogs
	  unlock_specialized_cultivation
	  pair_gather_01
      pair_gather_02
      pair_gather_03
	  gather_lumbering_bowsaw
	  gather_lumbering_twomansaw
	  gather_capacity_wheelbarrow
	  gather_capacity_handcarts
    </Technologies>
  </Researcher>
  <ResourceDropsite>
    <Types>food wood stone metal</Types>
  </ResourceDropsite>
  <StatusBars>
    <HeightOffset>9</HeightOffset>
  </StatusBars>
  <TerritoryDecay disable=""/>
  <TerritoryInfluence disable=""/>
  <Upgrade>
    <Pack>
      <Entity>units/scyth/support_wagon</Entity>
      <Time>10</Time>
    </Pack>
  </Upgrade>
  <VisualActor>
    <ConstructionPreview disable=""/>
    <Actor>structures/scythians/storehouse.xml</Actor>
    <FoundationActor>structures/scythians/storehouse_foundation.xml</FoundationActor>
  </VisualActor>
</Entity>
